# Roboflow Configuration
# Copy this file to .env and fill in your actual values

# Your Roboflow API key (get from https://app.roboflow.com/account)
ROBOFLOW_API_KEY=your_api_key_here

# Your Roboflow workspace name (found in the URL: https://app.roboflow.com/{workspace_name}/{project_name})
ROBOFLOW_WORKSPACE=your_workspace_name

# Your Roboflow project name (found in the URL: https://app.roboflow.com/{workspace_name}/{project_name})
ROBOFLOW_PROJECT=your_project_name

# Your Roboflow model version number (usually 1, 2, 3, etc.)
ROBOFLOW_VERSION=1
